{
  "{{keyForName}}": "{{page_name.paramCase()}}{{#isWf}}-{{pageSuffix.paramCase()}}{{/isWf}}",
  "{{keyForPage}}": {{#isDefault}}{
    "type": "neo_scaffold",
    "args": {
      "useSafeArea": true,
      "enableDeviceBack": true,
      "appBar": {
        "type": "neo_app_bar",
        "args": {
          "title": "An example title",
          "leftWidgetType": "NeoAppBarLeftWidgetType.backButton"
        }
      },
      "body": {
        "type": "neo_parallel_request_processor",
        "args": {
          "requestList": [],
          "child": {
            "type": "column",
            "args": {
              "crossAxisAlignment": "stretch",
              "children": [
                {
                  "type": "expanded",
                  "args": {
                    "child": {
                      "type": "single_child_scroll_view",
                      "args": {
                        "child": {
                          "type": "neo_text",
                          "args": {
                            "text": "This file is generated by a brick!"
                          }
                        }
                      }
                    }
                  }
                },
                {
                  "type": "neo_button",
                  "args": {
                    "labelText": "general_continue_button",
                    "size": "NeoButtonSize.large",
                    "displayMode": "NeoButtonDisplayMode.primary",
                    "formValidationRequired": true,
                    "padding": {
                      "top": 12,
                      "bottom": 20,
                      "end": 24,
                      "start": 24
                    }
                  }
                }
              ]
            }
          }
        }
      }
    }
  }{{/isDefault}}{{#isPopup}}{
    "type": "neo_popup",
      "args": {
        "type": "NeoPopupType.warning",
        "titleText": "An example confirmation title",
        "bodyText": "An example body text",
        "actions": [
          {
            "dismissOnAction": true,
            "labelText": "Confirmation button label",
            "transitionId": ""
          },
          {
            "labelText": "Cancel button label",
            "displayMode": "line",
            "transitionId": ""
          }
        ]
      }
  }{{/isPopup}}{{#isSuccess}}{
    "type": "neo_flow_completion_page",
    "args": {
      "useSafeArea": true,
      "rebuildWhenPopped": true,
      "backWidgetEventKey": "globalNavigationPopUntil",
      "backWidgetEventValue": "home",
      "appBar": {
        "type": "neo_app_bar",
        "args": {
          "title": "An example success page title",
          "actions": [
            {
              "type": "neo_icon_button",
              "args": {
                "iconUrn": "urn:local:icons:close_24px:svg",
                "navigationPath": "home",
                "navigationType": "NeoNavigationType.pop-until"
              }
            }
          ]
        }
      },
      "content": {
        "type": "neo_flow_completion_card",
        "args": {
          "image": {
            "type": "neo_image",
            "args": {
              "imageUrn": "urn:local:images:debit_success_image:svg",
              "showGridBackground": true
            }
          },
          "titleText": {
            "type": "neo_text",
            "args": {
              "text": "An example card title",
              "textAlign": "center",
              "textColor": "NeoTheme.neutralForeground1Rest",
              "style": "labelEighteenSemibold"
            }
          },
          "descriptionTextList": [
            {
              "type": "neo_text",
              "args": {
                "text": "Example card description",
                "textAlign": "center",
                "textColor": "NeoTheme.neutralForeground2Rest",
                "style": "labelFourteenMedium"
              }
            }
          ]
        }
      },
      "buttonList": [
        {
          "type": "neo_button",
          "args": {
            "navigationPath": "home",
            "navigationType": "NeoNavigationType.push-as-root",
            "labelText": "general_returnDashboard_button",
            "size": "NeoButtonSize.large"
          }
        }
      ],
      "padding": [
        24,
        20
      ]
    }
  }{{/isSuccess}}{{#isFailure}}{
    "type": "neo_flow_completion_page",
    "args": {
      "backWidgetEventKey": "globalNavigationPopUntil",
      "backWidgetEventValue": "home",
      "rebuildWhenPopped": true,
      "appBar": {
        "type": "neo_app_bar",
        "args": {
          "title": "An example failure page title",
          "actions": [
            {
              "type": "neo_icon_button",
              "args": {
                "iconUrn": "urn:local:icons:close_24px:svg",
                "navigationPath": "home",
                "navigationType": "NeoNavigationType.pop-until"
              }
            }
          ]
        }
      },
      "content": {
        "type": "column",
        "args": {
          "children": [
            {
              "type": "neo_flow_completion_card",
              "args": {
                "image": {
                  "type": "neo_image",
                  "args": {
                    "imageUrn": "urn:local:images:debit_fail_image:svg",
                    "showGridBackground": true
                  }
                },
                "titleText": {
                  "type": "neo_text",
                  "args": {
                    "text": "Example card title",
                    "textAlign": "center",
                    "textColor": "NeoTheme.neutralForeground1Rest",
                    "style": "labelEighteenSemibold"
                  }
                },
                "descriptionTextList": [
                  {
                    "type": "neo_text",
                    "args": {
                      "text": "Example card description",
                      "textAlign": "center",
                      "textColor": "NeoTheme.neutralForeground2Rest",
                      "style": "labelFourteenMedium"
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      "buttonList": [
        {
          "type": "neo_button",
          "args": {
            "navigationPath": "home",
            "navigationType": "NeoNavigationType.push-as-root",
            "labelText": "general_returnDashboard_button",
            "size": "NeoButtonSize.large"
          }
        }
      ],
      "padding": [
        24,
        20
      ]
    }
  }{{/isFailure}}
}