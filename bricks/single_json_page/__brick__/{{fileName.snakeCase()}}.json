{
  "{{keyForName}}": "{{page_name.paramCase()}}{{#isWf}}-{{pageSuffix.paramCase()}}{{/isWf}}",
  "{{keyForPage}}": {{#isDefault}}{
    "type": "neo_scaffold",
    "args": {
      "useSafeArea": true,
      "enableDeviceBack": true,
      "appBar": {
        "type": "neo_app_bar",
        "args": {
          "title": "An example title",
          "leftWidgetType": "NeoAppBarLeftWidgetType.backButton"
        }
      },
      "body": {
        "type": "neo_parallel_request_processor",
        "args": {
          "requestList": [],
          "child": {
            "type": "column",
            "args": {
              "crossAxisAlignment": "stretch",
              "children": [
                {
                  "type": "expanded",
                  "args": {
                    "child": {
                      "type": "single_child_scroll_view",
                      "args": {
                        "child": {
                          "type": "neo_text",
                          "args": {
                            "text": "This file is generated by a brick!"
                          }
                        }
                      }
                    }
                  }
                },
                {
                  "type": "neo_button",
                  "args": {
                    "labelText": "general_continue_button",
                    "size": "NeoButtonSize.large",
                    "displayMode": "NeoButtonDisplayMode.primary",
                    "formValidationRequired": true,
                    "padding": {
                      "top": 12,
                      "bottom": 20,
                      "end": 24,
                      "start": 24
                    }
                  }
                }
              ]
            }
          }
        }
      }
    }
  }{{/isDefault}}{{#isPopup}}{
    "type": "neo_popup",
      "args": {
        "type": "NeoPopupType.warning",
        "titleText": "An example confirmation title",
        "bodyText": "An example body text",
        "actions": [
          {
            "dismissOnAction": true,
            "labelText": "Confirmation button label",
            "transitionId": ""
          },
          {
            "labelText": "Cancel button label",
            "displayMode": "line",
            "transitionId": ""
          }
        ]
      }
  }{{/isPopup}}{{#isSuccess}}{
    "type": "neo_scaffold",
    "args": {
      "enableDeviceBack": true,
      "useSafeArea": true,
      "appBar": {
        "type": "neo_app_bar",
        "args": {
          "title": "An example success page title",
          "actions": [
            {
              "type": "neo_icon_button",
              "args": {
                "iconUrn": "urn:local:icons:close_24px:svg",
                "navigationPath": "home",
                "navigationType": "NeoNavigationType.push-as-root"
              }
            }
          ]
        }
      },
      "body": {
        "type": "column",
        "args": {
          "crossAxisAlignment": "stretch",
          "children": [
            {
              "type": "expanded",
              "args": {
                "child": {
                  "type": "single_child_scroll_view",
                  "args": {
                    "child": {
                      "type": "neo_flow_completion_card",
                      "args": {
                        "padding": {
                          "top": 20,
                          "start": 24,
                          "end": 24
                        },
                        "image": {
                          "type": "neo_image",
                          "args": {
                            "imageUrn": "urn:local:images:success_image:svg",
                            "showGridBackground": true
                          }
                        },
                        "titleText": {
                          "type": "neo_text",
                          "args": {
                            "text": "profileAndSettings_otherDocumentTransactionsSuccessPage_email_title",
                            "textAlign": "center",
                            "textColor": "NeoColors.textDefault",
                            "style": "labelEighteenSemibold"
                          }
                        },
                        "descriptionTextList": [
                          {
                            "type": "neo_text",
                            "args": {
                              "text": "profileAndSettings_otherDocumentTransactionsSuccessPage_info_text",
                              "textAlign": "center",
                              "textColor": "NeoColors.textSecondary",
                              "style": "labelFourteenMedium"
                            }
                          }
                        ]
                      }
                    }
                  }
                }
              }
            },
            {
              "type": "neo_button",
              "args": {
                "navigationPath": "home",
                "navigationType": "NeoNavigationType.push-as-root",
                "labelText": "general_returnDashboard_button",
                "size": "NeoButtonSize.large",
                "padding": {
                  "top": 12,
                  "bottom": 20,
                  "start": 24,
                  "end": 24
                }
              }
            }
          ]
        }
      }
    }
  }{{/isSuccess}}{{#isFailure}}{
    "type": "neo_scaffold",
    "args": {
      "enableDeviceBack": true,
      "useSafeArea": true,
      "appBar": {
        "type": "neo_app_bar",
        "args": {
          "title": "An example failure page title",
          "actions": [
            {
              "type": "neo_icon_button",
              "args": {
                "iconUrn": "urn:local:icons:close_24px:svg",
                "navigationPath": "home",
                "navigationType": "NeoNavigationType.push-as-root"
              }
            }
          ]
        }
      },
      "body": {
        "type": "column",
        "args": {
          "crossAxisAlignment": "stretch",
          "children": [
            {
              "type": "expanded",
              "args": {
                "child": {
                  "type": "single_child_scroll_view",
                  "args": {
                    "child": {
                      "type": "neo_flow_completion_card",
                      "args": {
                        "padding": {
                          "top": 20,
                          "start": 24,
                          "end": 24
                        },
                        "image": {
                          "type": "neo_image",
                          "args": {
                            "imageUrn": "urn:local:images:debit_succes_error_image:svg",
                            "showGridBackground": true
                          }
                        },
                        "titleText": {
                          "type": "neo_text",
                          "args": {
                            "text": "An example failure title",
                            "textAlign": "center",
                            "textColor": "NeoColors.textDefault",
                            "style": "labelEighteenSemibold"
                          }
                        },
                        "descriptionTextList": [
                          {
                            "type": "neo_text",
                            "args": {
                              "text": "An example failure description",
                              "textAlign": "center",
                              "textColor": "NeoColors.textSecondary",
                              "style": "labelFourteenMedium"
                            }
                          }
                        ]
                      }
                    }
                  }
                }
              }
            },
            {
              "type": "neo_button",
              "args": {
                "navigationPath": "home",
                "navigationType": "NeoNavigationType.push-as-root",
                "labelText": "general_returnDashboard_button",
                "size": "NeoButtonSize.large",
                "padding": {
                  "top": 12,
                  "bottom": 20,
                  "start": 24,
                  "end": 24
                }
              }
            }
          ]
        }
      }
    }
  }{{/isFailure}}
}